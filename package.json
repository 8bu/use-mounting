{
  "name": "@8bu/use-mounting",
  "version": "0.0.1-beta.2",
  "packageManager": "pnpm@6.32.3",
  "description": "Programmatic mount your Vue components",
  "keywords": [
    "vue",
    "programmatic",
    "mount"
  ],
  "license": "MIT",
  "author": "8bu <nhvlongg@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/8bu/use-mounting.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pkgroll",
    "dev": "pkgroll --watch",
    "play": "vite playground --open",
    "play:build": "vite build playground",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:e2e": "cypress open",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "dependencies": {
    "@vueuse/core": "^8.2.5",
    "vue": "^3.2.32"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@antfu/ni": "^0.14.0",
    "@types/node": "^17.0.23",
    "bumpp": "^7.1.1",
    "cypress": "^9.5.4",
    "eslint": "^8.13.0",
    "nanoid": "^3.3.2",
    "pkgroll": "^1.0.3",
    "pnpm": "^6.32.6",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vitest": "^0.9.3",
    "vue-router": "^4.0.14",
    "vue-tsc": "~0.34.6"
  }
}
